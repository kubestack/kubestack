{
  "id": "jenkins-slave",
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "jenkins-slave",
    "labels": {
      "name": "jenkins-slave"
    }
  },
  "spec": {
      "ports": [
          {
            "port": 50000,
            "containerPort": 50000
          }
      ]
  },
  "selector": {
    "name": "jenkins"
  },
  "createExternalLoadBalancer": true
}

